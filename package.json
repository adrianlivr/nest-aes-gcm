{
  "name": "@adrianlivr/nest-aes-gcm",
  "version": "1.0.0",
  "description": "Encryption AES_GCM module for NestJS framework",
  "author": "Adriano Oliveira",
  "main": "dist/index.js",
  "files": [
    "dist/"
  ],
  "license": "MIT",
  "scripts": {
    "build": "tsc -p tsconfig.build.json",
    "clean": "rm -rf dist",
    "dev": "tsc --ts watch",
    "test": "jest",
    "test:cov": "jest --coverage",
    "lint": "eslint 'src/**/*.ts' --fix",
    "prepublish": "npm run clean && npm run build"
  },
  "devDependencies": {
    "@nestjs/core": "^7.6.4",
    "@nestjs/testing": "^7.6.4",
    "@types/jest": "^26.0.19",
    "@types/node": "^14.14.7",
    "@typescript-eslint/eslint-plugin": "^4.11.0",
    "@typescript-eslint/parser": "^4.10.0",
    "eslint": "^7.16.0",
    "eslint-config-prettier": "^7.1.0",
    "eslint-plugin-import": "^2.22.1",
    "jest": "^26.6.3",
    "prettier": "^2.2.1",
    "reflect-metadata": "^0.1.13",
    "rxjs": "^6.6.3",
    "ts-jest": "^26.4.4",
    "ts-node": "^9.0.0",
    "typescript": "^4.0.5"
  },
  "dependencies": {
    "@nestjs/common": "^7.6.4"
  },
  "jest": {
    "moduleFileExtensions": [
      "js",
      "json",
      "ts"
    ],
    "rootDir": "src",
    "testRegex": ".spec.ts$",
    "transform": {
      "^.+\\.(t|j)s$": "ts-jest"
    },
    "coverageDirectory": "../coverage",
    "testEnvironment": "node"
  },
  "repository": {
    "type": "git",
    "url": "https://github.com/adrianlivr/nest-aes-gcm"
  }
}
